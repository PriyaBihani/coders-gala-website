s: state.speciality,\n});\n\nexport default connect(mapStateToProps, { getSpeciality })(Article);\n","/Users/kartikgupta/Desktop/CG-piarepo/coders-gala-website/client/src/components/UpsertCard.js",["280","281"],"/Users/kartikgupta/Desktop/CG-piarepo/coders-gala-website/client/src/components/UpsertArticle.js",[],"/Users/kartikgupta/Desktop/CG-piarepo/coders-gala-website/client/src/components/About.js",[],"/Users/kartikgupta/Desktop/CG-piarepo/coders-gala-website/client/src/components/Learn.js",[],"/Users/kartikgupta/Desktop/CG-piarepo/coders-gala-website/client/src/components/SignIn.js",["282"],"import React, { useState, useEffect } from 'react';\nimport { Redirect, NavLink } from 'react-router-dom';\nimport { connect } from 'react-redux';\nimport { login } from '../actions/auth';\nimport { Seo } from '../helpers';\n\nconst SignIn = ({ login, token }) => {\n  const [email, setEmail] = useState('');\n  const [password, setPassword] = useState('');\n\n  if (token) return <Redirect to=\"/learn\" />;\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    document.querySelector('.section h1').classList.add('changed');\n    console.log({ email, password });\n    login({ email, password });\n  };\n\n  const dinText = '<Coders Gala/>';\n\n  return (\n    <div className=\"auth-container\">\n      <Seo title=\"LogIn\" />\n      <div className=\"container\">\n        <div className=\"form-container\">\n          <div className=\"section\">\n            <h6 className=\"dinTag\">{dinText}</h6>\n            <h1>Log in</h1>\n            <p>Hey, Welcome Back !!</p>\n            <form onSubmit={handleSubmit}>\n              <div className=\"form-field\">\n                <label htmlFor=\"email\">Email</label>\n                <input\n                  onChange={(e) => {\n                    setEmail(e.target.value);\n                  }}\n                  type=\"email\"\n                  id=\"email\"\n                  className=\"user-input\"\n                  required\